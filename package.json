{
  "name": "trinity-react-template",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonluca/vite-typescript-ssr-react"
  },
  "license": "MIT",
  "author": "Arun Manohar Manoharan",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.prod.json && pnpm build:client && pnpm build:server && pnpm copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets",
    "dev:client": "pnpm build:client && vite --config vite.config.ts dev",
    "dev:server": "nodemon --watch server.ts --watch src/server --exec 'ts-node server.ts'",
    "format": "prettier --write .",
    "prepare": "husky install",
    "serve": "pnpm build && cross-env NODE_ENV=production pnpm ts-node ./server",
    "serve:local": "vite serve",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,scss}": [
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.3",
    "autoprefixer": "^10.4.14",
    "compression": "1.7.4",
    "cross-env": "^7.0.3",
    "css-mediaquery": "^0.1.2",
    "date-fns": "^2.30.0",
    "express": "4.18.2",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "ts-node": "^10.9.1",
    "ua-parser-js": "^1.0.35"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/compression": "1.7.2",
    "@types/concurrently": "6.4.0",
    "@types/css-mediaquery": "^0.1.1",
    "@types/eslint": "8.40.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/serve-static": "^1.15.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "8.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.1",
    "serve-static": "^1.15.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
